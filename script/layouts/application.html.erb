<!DOCTYPE html>
<html>
<head>
  <title>Rebase</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>



<body>
<dl class="menu">
	<% if user_signed_in? %>
		<dt><%= link_to 'Ask', '/ask' %></dt><dd>Ask a question</dd>
		<dt><%= link_to 'Sign out', '/users/sign_out' %></dt><dd>Logout</dd>
	<% else %>
		<dt><%= link_to 'Signup', '/users/sign_up' %></dt><dd>Get registered!</dd>
		<dt><%= link_to 'Sign in', '/users/sign_in' %></dt><dd>Login</dd>
	<% end %>
</dl>
	<p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>

<%= yield %>

 

</body>
</html>
